    {% endfor %}{}],
    "html":[{% for post in site.categories.html %}
        {"title" : "{{ post.title }}","url" : "{{ site.url }}{{ post.url }}","date" : "{{ post.date | date: "%Y-%m-%d" }}"},
    {% endfor %}{}],
    "links":[{% for post in site.categories.links %}
        {"title" : "{{ post.title }}","url" : "{{ site.url }}{{ post.url }}","date" : "{{ post.date | date: "%Y-%m-%d" }}"},
    {% endfor %}{}],
    "performance":[{% for post in site.categories.performance %}
